@using Savonia.Measurements.Models;
@model Savonia.Measurements.Models.MeasurementQueryModel

@{
    ViewBag.Title = "Export  Enter";
    IEnumerable<SensorModel> sensors = ViewBag.Sensors as IEnumerable<SensorModel>;  
}

    <div class="jumbotron span4 offset4 text-center">
        <h1>Export</h1>
        <p class="lead">Savonia Measurements</p>
    </div>
<div class="span4 offset4 text-center">       
    <form action="@Url.Action("Login")" method="post" id="loginSubmit">
        @Html.AntiForgeryToken()
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <h1>Enter Key</h1>
                <div class="form-horizontal">

                    <div class="form-group">
                       
                        <div class="col-md-12">
                            @Html.EditorFor(model => model.Key, new { htmlAttributes = new { @class = "form-control" ,@style = "display: inline-block;"} })
                        </div>
                        @Html.ValidationMessageFor(model => model.Key, "", new { @class = "text-danger" })
                        <div class="col-md-12">
                            <button type="submit" class="btn btn-default" style="margin-top:4px">Enter</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>


@section head {
    @Styles.Render("~/Content/jquery-ui-1.11.4.custom/cssjqueryui")
}

@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryui")
}